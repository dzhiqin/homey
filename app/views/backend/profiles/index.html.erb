<div class="">
  <h1>员工列表</h1>
  <table class="table table-bg">
    <thead>
      <th>姓名</th>
      <th>邮箱</th>
      <th>职位</th>
      <th>操作</th>
    </thead>
    <tbody>
      <% @users.each do |e_user| %>
      <tr>
        <td><%= e_user.profile.name %></td>
        <td><%= e_user.email %></td>
        <td><%= e_user.profile.job %></td>
        <td><%= link_to '编辑',edit_backend_profile_path(e_user.profile) %> |
          <%= link_to "工资条",backend_user_salaries_path(e_user) %>
        </td>

      </tr>
      <% end %>
    </tbody>
  </table>
</div>
<%= paginate @users %>
